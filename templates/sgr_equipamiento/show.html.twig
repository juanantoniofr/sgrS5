{% extends 'base.html.twig' %}

{% block title %}SgrEquipamiento{% endblock %}

{% block body %}
    <h1>Equipamiento</h1>

    <table class="table table-striped">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ sgr_equipamiento.id }}</td>
            </tr>
            <tr>
                <th>Nombre</th>
                <td>{{ sgr_equipamiento.nombre }}</td>
            </tr>
            <tr>
                <th>Descripcion</th>
                <td>{{ sgr_equipamiento.descripcion }}</td>
            </tr>
        </tbody>
    </table>

    <div class="mb-4">

        <a class="btn btn-primary mb-2" role="button" href="{{ path('sgr_equipamiento_edit', {'id': sgr_equipamiento.id}) }}">Editar</a><br />
        <a href="{{ path('sgr_equipamiento_index') }}">Volver a la lista</a>
    </div>
    {{ include('sgr_equipamiento/_delete_form.html.twig') }}
{% endblock %}

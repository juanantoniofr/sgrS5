    <script type="text/javascript" src="/build/datetimepickerInit.js"></script>
    <script type="text/javascript" src="/build/datetimepicker-eventoViews.js"></script>
    -->
     
    <script type="text/javascript" src="/reservasfgh/public/build/datetimepickerInit.js"></script>
    <script type="text/javascript" src="/reservasfgh/public/build/datetimepicker-eventoViews.js"></script>
    
{% endblock %}